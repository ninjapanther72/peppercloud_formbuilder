{
  "name": "peppercloud_formbuilder_frontend",
  "version": "0.1.0",
  "private": true,
  "author": "Sunil Choudhary",
  "description": "This project is a full-stack MERN application that allows users to create, edit, view, and manage forms. The application will be built using React.js for the frontend, Node.js with Express.js for the backend, MongoDB for the database, and will be deployed using Netlify (frontend), Render (backend), and MongoDB Atlas (cloud database).",
  "proxy": "http://localhost:8030",
  "scripts": {
    "start": "set PORT=3030 && react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "fronted-build": "react-scripts build",
    "frontend-serve-build": "serve -s build",
    "fronted-netlify-build-deploy": "react-scripts build && netlify deploy --prod --dir=build",
    "fronted-netlify-build-login-deploy": "react-scripts build && netlify login && netlify link && netlify deploy --prod --dir=build",
    "fronted-netlify-login": "netlify login && netlify link",
    "install:clean": "rm -rf node_modules/ && rm -rf package-lock.json && npm install && npm start",
    "install:peer-deps": "npm i --legacy-peer-deps"
  },
  "dependencies": {
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "cjs-loader": "^0.1.0",
    "classnames": "^2.3.2",
    "i": "^0.3.7",
    "moment": "^2.29.4",
    "netlify-cli": "^17.34.2",
    "popper.js": "^1.16.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-redux": "^9.1.0",
    "react-router-dom": "6.3.0",
    "react-scripts": "5.0.1",
    "web-vitals": "2.1.4"
  },
  "devDependencies": {
    "@babel/core": "^7.22.5",
    "babel-loader": "^9.1.2",
    "css-loader": "^6.8.1",
    "eslint": "8.17.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-flowtype": "^8.0.3",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-react": "7.30.0",
    "eslint-plugin-react-hooks": "4.5.0",
    "html-webpack-plugin": "^5.5.3",
    "mini-css-extract-plugin": "^2.7.6",
    "webp-loader": "^0.6.0",
    "webpack": "^5.88.1",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "engines": {
    "node": "14 || 15 || 16",
    "npm": ">=6"
  }
}
